//一个数列: 8，4，2，1，23，344，12
//循环输出数列的值
//求数列中所有数值的和
//猜数游戏:从键盘中任意输入一个数据，判断数列中是否包含此数

import java.util.Scanner;

import java.util.Scanner;

public class world2 {
        public void main(String[] args) {
                Scanner scaner = new Scanner(System.in);
                int[] arr = {8, 4, 2, 1, 23, 344, 12};//定义数组
                int sum = 0;
                for (int i = 0; i < arr.length; i++) {
                        System.out.println(arr[i]);
                        sum+=arr[i];
                }

                System.out.println("sum:" + sum);//数值求和

                System.out.println("任意输入一个数据");
                int sq = scaner.nextInt();//从键盘中任意输入一个数据
                boolean duicuo = false;
                for (int i = 0; i < arr.length; i++) {
                        if (sq == arr[i]) {
                                duicuo = true;
                                break;
                        }
                }
                System.out.println(duicuo ? "zhaodao" : "mozhaodao");
        }
}
